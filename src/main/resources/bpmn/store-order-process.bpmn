        <camunda:formData>
          <camunda:formField id="customerEmail" label="Customer Email" type="string" defaultValue="customer@a.a" />
          <camunda:formField id="storeId" label="Store ID" type="string" defaultValue="store1" />
          <camunda:formField id="itemId" label="Item ID" type="long" defaultValue="1" />
        </camunda:formData>
        <camunda:executionListener delegateExpression="${createOrderDelegate}" event="end" />
      </bpmn:extensionElements>
      <bpmn:outgoing>Flow_1txrq5g</bpmn:outgoing>
    </bpmn:startEvent>
    <bpmn:userTask id="Activity_StoreProcessOrder" name="Store Process Order" camunda:assignee="${storeId}">
      <bpmn:documentation>You need to process order ${orderId}.</bpmn:documentation>
      <bpmn:extensionElements>
        <camunda:formData>
          <camunda:formField id="action" label="Action" type="enum" defaultValue="collect">
            <camunda:value id="collect" name="Collect" />
            <camunda:value id="cancel" name="Cancel" />
          </camunda:formField>
          <camunda:formField id="orderId" label="Order ID" type="long" defaultValue="${orderId}">
            <camunda:properties />
            <camunda:validation>
              <camunda:constraint name="readonly" config="true" />
            </camunda:validation>
          </camunda:formField>
        </camunda:formData>
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_1txrq5g</bpmn:incoming>
      <bpmn:outgoing>Flow_0vle57u</bpmn:outgoing>
    </bpmn:userTask>
    <bpmn:sequenceFlow id="Flow_1txrq5g" sourceRef="StartEvent_OrderStart" targetRef="Activity_StoreProcessOrder">
      <bpmn:extensionElements />
    </bpmn:sequenceFlow>
    <bpmn:exclusiveGateway id="Gateway_OrderAction" name="Order Action?">
      <bpmn:incoming>Flow_0vle57u</bpmn:incoming>
      <bpmn:outgoing>Flow_Collect</bpmn:outgoing>
      <bpmn:outgoing>Flow_Cancel</bpmn:outgoing>
    </bpmn:exclusiveGateway>
    <bpmn:sequenceFlow id="Flow_0vle57u" sourceRef="Activity_StoreProcessOrder" targetRef="Gateway_OrderAction" />
    <bpmn:sequenceFlow id="Flow_Collect" name="Collect" sourceRef="Gateway_OrderAction" targetRef="Activity_CollectOrder">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">${action == 'collect'}</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    <bpmn:sequenceFlow id="Flow_Cancel" name="Cancel" sourceRef="Gateway_OrderAction" targetRef="Activity_CancelOrder">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">${action == 'cancel'}</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    <bpmn:serviceTask id="Activity_CancelOrder" name="Cancel Order" camunda:delegateExpression="${cancelOrderDelegate}">
      <bpmn:incoming>Flow_Cancel</bpmn:incoming>
      <bpmn:outgoing>Flow_0mj2hla</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:endEvent id="Event_OrderCancelled" name="Order Cancelled">
      <bpmn:incoming>Flow_0mj2hla</bpmn:incoming>
    </bpmn:endEvent>
    <bpmn:sequenceFlow id="Flow_0mj2hla" sourceRef="Activity_CancelOrder" targetRef="Event_OrderCancelled" />
    <bpmn:serviceTask id="Activity_CollectOrder" name="Collect Order" camunda:delegateExpression="${collectOrderDelegate}">
      <bpmn:incoming>Flow_Collect</bpmn:incoming>
      <bpmn:outgoing>Flow_0lkap36</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:endEvent id="Event_OrderCollected" name="Order Collected">
      <bpmn:incoming>Flow_0lkap36</bpmn:incoming>
    </bpmn:endEvent>
    <bpmn:sequenceFlow id="Flow_0lkap36" sourceRef="Activity_CollectOrder" targetRef="Event_OrderCollected" />
  </bpmn:process>
